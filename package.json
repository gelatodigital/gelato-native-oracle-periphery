{
  "name": "gelato-native-oracle-periphery",
  "version": "1.0.0",
  "description": "Periphery Gelato Native Oracle L1 Smart Contract & Web3Function",
  "scripts": {
    "build": "yarn install && yarn compile && npx tsc",
    "clean": "yarn hardhat clean && rm -rf dist",
    "compile": "npx hardhat compile --force",
    "deploy": "npx hardhat deploy",
    "test": "npx hardhat test",
    "format": "prettier --write '*/**/*.*{js,json,md,ts}'",
    "format:check": "prettier --check '*/**/*.*{js,json,md,ts}'",
    "lint": "eslint --cache . && yarn lint:sol",
    "lint:ts": "eslint -c .eslintrc.json --ext \"**/*.ts\" \"**/*.test.ts\"",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "postinstall": "yarn husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@gelatonetwork/automate-sdk": "3.0.21",
    "@gelatonetwork/web3-functions-sdk": "2.4.4",
    "@nomicfoundation/hardhat-ethers": "3.0.6",
    "@openzeppelin/contracts": "5.0.2",
    "@pythnetwork/pyth-evm-js": "1.61.0",
    "@pythnetwork/pyth-sdk-solidity": "3.1.0",
    "@tsconfig/recommended": "1.0.7",
    "@typechain/ethers-v6": "0.5.1",
    "@typechain/hardhat": "9.1.0",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "dotenv": "16.4.5",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "ethers": "6.13.2",
    "hardhat": "2.22.8",
    "hardhat-deploy": "0.12.4",
    "husky": "8.0.3",
    "ky": "1.2.2",
    "lint-staged": "14.0.1",
    "prettier": "2.8.8",
    "prettier-plugin-solidity": "1.1.3",
    "solhint": "3.6.2",
    "solhint-plugin-prettier": "0.0.5",
    "ts-node": "10.9.2",
    "typechain": "8.3.2",
    "typescript": "5.5.4"
  },
  "lint-staged": {
    "*.*{js,json,md,ts,sol,yml,yaml}": "prettier --write",
    "*.*{ts,js}": "eslint -c .eslintrc.json"
  }
}
